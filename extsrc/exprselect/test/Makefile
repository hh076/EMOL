EXES = es
OBJS = main.o
all: $(EXES) 
LIBFILE =    
LIBDIR = .

######################################
######################################
######################################
CC = gcc 
LD = gcc 
    
CCFLAGS = -Wall -fPIC -g -I.
LDFLAGS = -Wall -fPIC -g #-L$(LIBDIR) -lblas -llapack
    
######################################
###################################### 
###################################### 
    
.SUFFIXES: .c .o
.c.o:   
	$(CC) $(CCFLAGS) -c $<
    
exe: $(EXES)

es: $(OBJS)
	$(LD) $(LDFLAGS) -o es $(OBJS)
    
######################################
######################################
######################################
clean:  
	-rm $(OBJS) $(EXES)

